
     server {
        listen       80;
        # server_name  142.93.73.66;
  
        location / {
           proxy_cache                     off;
           proxy_pass http://10.10.10.254:10000/;
           proxy_redirect     off;
   
           #http://$host
           proxy_set_header   Host $host;
           sub_filter http://10.10.10.254:10000/ http://$host/;
           sub_filter http://10.10.10.254:10000/ http://$host/;
           sub_filter_once off;
        } 
    }